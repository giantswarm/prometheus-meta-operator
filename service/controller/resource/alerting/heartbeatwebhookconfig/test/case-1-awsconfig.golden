metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: alertmanager
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: alertmanager
  name: alice
  namespace: monitoring
spec:
  receivers:
  - name: heartbeat
    webhookConfigs:
    - httpConfig:
        authorization:
          credentials:
            key: opsgenie.key
            name: new-alertmanager-config
          type: GenieKey
      sendResolved: false
      url: https://api.opsgenie.com/v2/heartbeats/test-installation-alice/ping
  route:
    groupInterval: 30s
    groupWait: 5m
    matchers:
    - name: cluster_id
      value: alice
    - name: installation
      value: test-installation
    - name: type
      value: heartbeat
    receiver: heartbeat
    repeatInterval: 1m
