apiVersion: v1beta1
kind: Ingress
metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: bar
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: bar
  namespace: bar-prometheus
spec:
  rules:
  - host: https://prometheus
    http:
      paths:
      - backend:
          serviceName: oauth2-proxy
          servicePort: 4180
        path: /bar/oauth2
  tls:
  - hosts:
    - https://prometheus
    secretName: prometheus-certificates
status:
  loadBalancer: {}
