apiVersion: v1beta1
kind: Ingress
metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: bob
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: bob
  namespace: bob-prometheus
spec:
  rules:
  - host: https://prometheus
    http:
      paths:
      - backend:
          serviceName: oauth2-proxy
          servicePort: 4180
        path: /bob/oauth2
  tls:
  - hosts:
    - https://prometheus
    secretName: prometheus-certificates
status:
  loadBalancer: {}
