apiVersion: v1
kind: Ingress
metadata:
  annotations:
    nginx.ingress.kubernetes.io/auth-realm: Authentication Required
    nginx.ingress.kubernetes.io/auth-secret: remote-write-ingress-auth
    nginx.ingress.kubernetes.io/auth-type: basic
    nginx.ingress.kubernetes.io/client-body-buffer-size: 50m
    nginx.ingress.kubernetes.io/proxy-body-size: 50m
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: bar
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
    giantswarm.io/cluster: bar
  name: prometheus-bar-remote-write
  namespace: mimir
spec:
  ingressClassName: nginx
  rules:
  - host: prometheus
    http:
      paths:
      - backend:
          service:
            name: mimir-gateway
            port:
              number: 80
        path: /bar/api/v1/push
        pathType: ImplementationSpecific
status:
  loadBalancer: {}
