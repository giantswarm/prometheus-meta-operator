metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: bob
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: bob
  namespace: bob-prometheus
spec:
  additionalAlertManagerConfigs:
    key: alertmanager-additional.yaml
    name: alertmanager-config
  additionalScrapeConfigs:
    key: prometheus-additional.yaml
    name: additional-scrape-configs
  apiserverConfig:
    host: https://master.bob
    tlsConfig:
      ca: {}
      caFile: /etc/prometheus/secrets/cluster-certificates/ca
      cert: {}
      certFile: /etc/prometheus/secrets/cluster-certificates/crt
      keyFile: /etc/prometheus/secrets/cluster-certificates/key
  arbitraryFSAccessThroughSMs: {}
  externalLabels:
    cluster_id: bob
    cluster_type: tenant_cluster
    customer: Giant Swarm
    installation: test-installation
    pipeline: testing
    provider: kvm
    region: onprem
  externalUrl: http://prometheus/bob
  podMetadata:
    labels:
      app.kubernetes.io/instance: bob
      app.kubernetes.io/managed-by: prometheus-meta-operator
      app.kubernetes.io/name: prometheus
      giantswarm.io/monitoring: "true"
  remoteWrite:
  - basicAuth:
      password:
        key: password
        name: remote-write
      username:
        key: username
        name: remote-write
    name: bob
    url: ""
    writeRelabelConfigs:
    - action: keep
      regex: (^aggregation:.+|prometheus_tsdb_head_series|^slo_.+)
      sourceLabels:
      - __name__
  replicas: 1
  resources:
    requests:
      cpu: 100m
      memory: 100Mi
  retention: 2w
  retentionSize: 45Gi
  routePrefix: /bob
  ruleSelector:
    matchLabels:
      cluster_id: bob
  rules:
    alert: {}
  secrets:
  - cluster-certificates
  securityContext:
    fsGroup: 2000
    runAsGroup: 65534
    runAsNonRoot: true
    runAsUser: 1000
  serviceMonitorSelector:
    matchLabels:
      cluster_id: bob
  storage:
    volumeClaimTemplate:
      metadata: {}
      spec:
        accessModes:
        - ReadWriteOnce
        resources:
          requests:
            storage: 50Gi
      status: {}
  walCompression: true
