metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: kubernetes
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: kubernetes
  namespace: kubernetes-prometheus
spec:
  additionalAlertManagerConfigs:
    key: alertmanager-additional.yaml
    name: alertmanager-config
  additionalScrapeConfigs:
    key: prometheus-additional.yaml
    name: additional-scrape-configs
  affinity:
    nodeAffinity:
      requiredDuringSchedulingIgnoredDuringExecution:
        nodeSelectorTerms:
        - matchExpressions:
          - key: role
            operator: NotIn
            values:
            - master
  apiserverConfig:
    bearerTokenFile: /var/run/secrets/kubernetes.io/serviceaccount/token
    host: https://
    tlsConfig:
      ca: {}
      caFile: /var/run/secrets/kubernetes.io/serviceaccount/ca.crt
      cert: {}
      insecureSkipVerify: true
  arbitraryFSAccessThroughSMs: {}
  externalLabels:
    cluster_id: kubernetes
    cluster_type: control_plane
    customer: Giant Swarm
    installation: test-installation
    pipeline: testing
    provider: kvm
    region: onprem
  externalUrl: http://prometheus/kubernetes
  podMetadata:
    labels:
      app.kubernetes.io/instance: kubernetes
      app.kubernetes.io/managed-by: prometheus-meta-operator
      app.kubernetes.io/name: prometheus
      giantswarm.io/monitoring: "true"
  priorityClassName: prometheus
  remoteWrite:
  - basicAuth:
      password:
        key: password
        name: remote-write
      username:
        key: username
        name: remote-write
    name: kubernetes
    url: http://grafana/api/prom/push
    writeRelabelConfigs:
    - action: keep
      regex: (^aggregation:.+|prometheus_tsdb_head_series|^slo_.+)
      sourceLabels:
      - __name__
  replicas: 1
  resources:
    limits:
      cpu: 100m
      memory: 1000Mi
    requests:
      cpu: 100m
      memory: 1000Mi
  retention: 2w
  retentionSize: 45Gi
  routePrefix: /kubernetes
  ruleNamespaceSelector:
    matchLabels:
      name: monitoring
  ruleSelector: {}
  rules:
    alert: {}
  secrets:
  - etcd-certificates
  securityContext:
    fsGroup: 2000
    runAsGroup: 65534
    runAsNonRoot: true
    runAsUser: 1000
  serviceMonitorSelector:
    matchLabels:
      cluster_id: kubernetes
  storage:
    volumeClaimTemplate:
      metadata: {}
      spec:
        accessModes:
        - ReadWriteOnce
        resources:
          requests:
            storage: 50Gi
      status: {}
  topologySpreadConstraints:
  - labelSelector:
      matchLabels:
        app: prometheus
    maxSkew: 1
    topologyKey: kubernetes.io/hostname
    whenUnsatisfiable: ScheduleAnyway
  walCompression: true
