metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: bar
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: bar
  namespace: bar-prometheus
spec:
  additionalAlertManagerConfigs:
    key: alertmanager-additional.yaml
    name: alertmanager-config
  additionalScrapeConfigs:
    key: prometheus-additional.yaml
    name: additional-scrape-configs
  affinity:
    nodeAffinity:
      preferredDuringSchedulingIgnoredDuringExecution:
      - preference:
          matchExpressions:
          - key: role
            operator: NotIn
            values:
            - master
        weight: 60
  apiserverConfig:
    host: https://master.bar
    tlsConfig:
      ca: {}
      caFile: /etc/prometheus/secrets/cluster-certificates/ca
      cert: {}
      certFile: /etc/prometheus/secrets/cluster-certificates/crt
      keyFile: /etc/prometheus/secrets/cluster-certificates/key
  arbitraryFSAccessThroughSMs: {}
  externalLabels:
    cluster_id: bar
    cluster_type: tenant_cluster
    customer: Giant Swarm
    installation: test-installation
    pipeline: testing
    provider: kvm
    region: onprem
  externalUrl: http://prometheus/bar
  podMetadata:
    labels:
      app.kubernetes.io/instance: bar
      app.kubernetes.io/managed-by: prometheus-meta-operator
      app.kubernetes.io/name: prometheus
      giantswarm.io/monitoring: "true"
  replicas: 1
  resources:
    requests:
      cpu: 100m
      memory: 100Mi
  retention: 2w
  retentionSize: 45Gi
  routePrefix: /bar
  ruleSelector:
    matchLabels:
      cluster_id: bar
  rules:
    alert: {}
  secrets:
  - cluster-certificates
  securityContext:
    fsGroup: 2000
    runAsGroup: 65534
    runAsNonRoot: true
    runAsUser: 1000
  serviceMonitorSelector:
    matchLabels:
      cluster_id: bar
  storage:
    volumeClaimTemplate:
      metadata: {}
      spec:
        accessModes:
        - ReadWriteOnce
        resources:
          requests:
            storage: 50Gi
      status: {}
  walCompression: true
