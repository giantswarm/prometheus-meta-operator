metadata:
  creationTimestamp: null
  labels:
    app.kubernetes.io/instance: alice
    app.kubernetes.io/managed-by: prometheus-meta-operator
    app.kubernetes.io/name: prometheus
  name: alice
  namespace: alice-prometheus
spec:
  additionalAlertManagerConfigs:
    key: alertmanager-additional.yaml
    name: alertmanager-config
  additionalScrapeConfigs:
    key: prometheus-additional.yaml
    name: additional-scrape-configs
  affinity:
    nodeAffinity:
      preferredDuringSchedulingIgnoredDuringExecution:
      - preference:
          matchExpressions:
          - key: role
            operator: NotIn
            values:
            - master
        weight: 60
  apiserverConfig:
    host: https://master.alice
    tlsConfig:
      ca: {}
      caFile: /etc/prometheus/secrets/cluster-certificates/ca
      cert: {}
      certFile: /etc/prometheus/secrets/cluster-certificates/crt
      keyFile: /etc/prometheus/secrets/cluster-certificates/key
  arbitraryFSAccessThroughSMs: {}
  externalLabels:
    cluster_id: alice
    cluster_type: tenant_cluster
    customer: Giant Swarm
    installation: test-installation
    pipeline: testing
    provider: kvm
    region: onprem
  externalUrl: http://prometheus/alice
  podMetadata:
    labels:
      app.kubernetes.io/instance: alice
      app.kubernetes.io/managed-by: prometheus-meta-operator
      app.kubernetes.io/name: prometheus
      giantswarm.io/monitoring: "true"
  remoteWrite:
  - basicAuth:
      password:
        key: password
        name: remote-write
      username:
        key: username
        name: remote-write
    name: alice
    url: http://grafana/api/prom/push
    writeRelabelConfigs:
    - action: keep
      regex: (^aggregation:.+|prometheus_tsdb_head_series|^slo_.+)
      sourceLabels:
      - __name__
  replicas: 1
  resources:
    limits:
      cpu: "1"
      memory: 5000Mi
    requests:
      cpu: "1"
      memory: 5000Mi
  retention: 2w
  retentionSize: 45Gi
  routePrefix: /alice
  ruleNamespaceSelector:
    matchLabels:
      name: monitoring
  ruleSelector:
    matchExpressions:
    - key: cluster_type
      operator: NotIn
      values:
      - control_plane
  rules:
    alert: {}
  secrets:
  - cluster-certificates
  securityContext:
    fsGroup: 2000
    runAsGroup: 65534
    runAsNonRoot: true
    runAsUser: 1000
  serviceMonitorSelector:
    matchLabels:
      cluster_id: alice
  storage:
    volumeClaimTemplate:
      metadata: {}
      spec:
        accessModes:
        - ReadWriteOnce
        resources:
          requests:
            storage: 50Gi
      status: {}
  walCompression: true
